{
  "apiBaseUrl": "https://api.telegram.org",
  "timeout": 45000,
  "retries": 5,
  "cacheTimeout": 300000,
  "pollingInterval": 3000,
  "maxMessageHistory": 100,
  "features": {
    "webhookInfo": true,
    "botCommands": true,
    "connectionTest": true,
    "autoRetry": true,
    "messaging": true,
    "polling": true,
    "fileDownload": true
  },
  "rateLimit": {
    "enabled": true,
    "requestsPerSecond": 20,
    "burstLimit": 30
  },
  "validation": {
    "strictMode": false,
    "checkBotPermissions": true,
    "verifyWebhookUrl": false,
    "quickValidation": true
  },
  "network": {
    "keepAlive": false,
    "keepAliveMsecs": 1000,
    "maxSockets": 10,
    "maxFreeSockets": 5
  },
  "messaging": {
    "autoStartPolling": false,
    "webhookEnabled": false,
    "fileDownloadPath": "./downloads",
    "messageRetentionDays": 30
  },
  "termux": {
    "enabled": true,
    "tlsWorkaround": true,
    "retryDelay": 2000,
    "maxRetries": 5
  }
}