{
  "name": "gemini",
  "displayName": "Google Gemini",
  "description": "Google Gemini AI API integration for chat and text generation",
  "version": "1.0.0",
  "author": "Credential Service",
  "enabled": true,
  "apiBaseUrl": "https://generativelanguage.googleapis.com",
  "timeout": 15000,
  "retries": 3,
  "cacheTimeout": 600000,
  "features": {
    "models": true,
    "chat": true,
    "connectionTest": true
  },
  "defaultModel": "gemini-2.5-flash",
  "supportedModels": [
    "gemini-2.5-pro",
    "gemini-2.5-flash",
    "gemini-2.5-flash-lite",
    "gemini-2.0-flash",
    "gemini-1.5-pro",
    "gemini-1.5-flash",
    "gemini-pro",
    "gemini-pro-vision"
  ],
  "maxTokens": 8192,
  "temperature": {
    "min": 0.0,
    "max": 2.0,
    "default": 0.7
  },
  "safetySettings": {
    "enabled": true,
    "thresholds": {
      "HARM_CATEGORY_HARASSMENT": "BLOCK_MEDIUM_AND_ABOVE",
      "HARM_CATEGORY_HATE_SPEECH": "BLOCK_MEDIUM_AND_ABOVE",
      "HARM_CATEGORY_SEXUALLY_EXPLICIT": "BLOCK_MEDIUM_AND_ABOVE",
      "HARM_CATEGORY_DANGEROUS_CONTENT": "BLOCK_MEDIUM_AND_ABOVE"
    }
  }
}
