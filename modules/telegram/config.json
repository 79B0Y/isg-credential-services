{
  "apiBaseUrl": "https://api.telegram.org",
  "timeout": 35000,
  "retries": 3,
  "cacheTimeout": 300000,
  "pollingInterval": 2000,
  "maxMessageHistory": 100,
  "features": {
    "webhookInfo": true,
    "botCommands": true,
    "connectionTest": true,
    "autoRetry": true,
    "messaging": true,
    "polling": true,
    "fileDownload": true
  },
  "rateLimit": {
    "enabled": true,
    "requestsPerSecond": 30,
    "burstLimit": 50
  },
  "validation": {
    "strictMode": true,
    "checkBotPermissions": true,
    "verifyWebhookUrl": false,
    "quickValidation": true
  },
  "network": {
    "keepAlive": true,
    "keepAliveMsecs": 1000
  },
  "messaging": {
    "autoStartPolling": false,
    "webhookEnabled": false,
    "fileDownloadPath": "./downloads",
    "messageRetentionDays": 30
  }
}